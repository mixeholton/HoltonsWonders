@page "/None"
@using MudBlazor

<h3>Side Ikke Fundet!</h3>

<div class="mt-2">
    <MudAlert Severity="Severity.Error" Variant="Variant.Filled" Class="d-flex justify-content-center w-100 align-content-center"> 
        @ErrorMessage
    </MudAlert>
</div>

@code {
    private string ErrorMessage { get; set; } = "Denne side findes ikke eller har du ikke rettigheder til den";    
}